@model Shopping.Data.Entities.City
@{
    ViewData["Title"] = "DetailsCity";
}

<h1>Detalles Ciudad</h1>

<div>
    <partial name="_CityDetails" model="Model"/>
</div>
<div>
    <a asp-action="EditCity" asp-route-id="@Model?.Id" class="btn btn-outline-warning">Editar ciudad</a>
    <a asp-action="DetailsState" asp-route-id="@Model?.State.Id" class="btn btn-outline-success">Regresar</a>
</div>



